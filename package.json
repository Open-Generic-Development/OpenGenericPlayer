{
  "name": "opengenericplayer",
  "version": "1.0.0",
  "description": "An open source cross-platform music player",
  "main": "main.js",
  "scripts": {
    "build-win": "electron-builder -w",
    "build-mac": "electron-builder -m",
    "build-lin": "electron-builder -l",
    "start": "electron .",
    "test": "echo 'Error: No tests at the moment'"
  },
  "repository": {
    "type": "gilamet",
    "url": "git+https://github.com/Open-Generic-Development/OpenGenericPlayer.git"
  },
  "author": "OpenGenericDevelopment (Christian B. / Florian S.)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Open-Generic-Development/OpenGenericPlayer/issues"
  },
  "build": {
    "appId": "systems.ogd.opengenericplayer",
    "mac": {
      "category": "public.app-category.music",
      "target": [
        {
          "target": "dmg"
        },
        {
          "target": "pkg"
        },
        {
          "target": "zip"
        }
      ],
      "icon": "img/icons/mac/icon.icns"
    },
    "win": {
      "icon": "img/icons/win/icon.ico",
      "target": [
        {
          "target": "nsis"
        },
        {
          "target": "portable"
        },
        {
          "target": "zip"
        }
      ]
    },
    "linux": {
      "icon": "img/icons/lin/512x512.png",
      "target": "AppImage",
      "category": "Audio"
    }
  },
  "homepage": "https://github.com/Open-Generic-Development/OpenGenericPlayer#readme",
  "dependencies": {
    "howler": "^2.2.3",
    "ytdl-core": "^4.8.3"
  },
  "devDependencies": {
    "electron-reload": "^1.5.0",
    "electron-builder": "^22.11.7",
    "electron": "^13.1.6"
  }
}
